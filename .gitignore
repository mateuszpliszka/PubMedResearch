# Auto detect text files and perform LF normalization
* text=auto

# Ignore all JSON and Parquet files
*.json
*.parquet

# Ignore files larger than 100MB
*.bigfile
maxsize=100MB

# Do not ignore folder structures
!Data/
!Data/0.Raw/
!Data/1.EarlyCleaned/
!Data/1.EarlyCleaned/cleaned_parquet/
!Data/1.EarlyCleaned/raw_parquet/
!Data/2.Processed/
!Data/2.Processed/EDA/
!Data/2.Processed/ModellingData/

# Ignore Python cache files
__pycache__/
*.pyc
